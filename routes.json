    {
      "api/ballot/mixin": {
        "method": "POST",
        "handler": "Mixin",
        "args": [
          {"election_id": "int"},
          {"ballot_id": "int"}
        ],
        "data-source": {
          "query": "SELECT e.election_id, e.title, b.ballot_id, b.ballot_location FROM voting.election e INNER JOIN ballot b ON e.election_id=b.election_id WHERE e.election_id=$1 AND b.ballot_id=$2"
